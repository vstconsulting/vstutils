{
    "compilerOptions": {
        "outDir": "./dist",
        "allowJs": true,
        "target": "esnext",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "module": "es2020",
        "moduleResolution": "node",
        "types": [
            "jquery",
            "bootstrap",
        ],
        "paths": {
            "@/*": ["./*"],
        }
    },
    "vueCompilerOptions": {
        "target": 2.7
    }
}
