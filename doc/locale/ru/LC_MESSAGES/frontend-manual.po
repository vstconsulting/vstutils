#: ../../frontend-manual.rst:345
msgid "**Example:**"
msgstr ""

#: ../../frontend-manual.rst:330
msgid ""
"*\"field-active-true\"* for model that contains \"boolean\" field with "
"name \"active\" and value \"true\""
msgstr ""

#: ../../frontend-manual.rst:331
msgid ""
"*\"field-tariff_type-WAREHOUSE\"* for model that contains \"choices\" "
"field with name \"tariff_type\" and value \"WAREHOUSE\""
msgstr ""

#: ../../frontend-manual.rst:336
msgid "``MultiActions`` - *selected__field-[field_name]-[field-value]*"
msgstr ""

#: ../../frontend-manual.rst:336
msgid ""
"*\"selected__field-tariff_type-WAREHOUSE\"* and *\"selected__field-"
"tariff_type-SLIDE\"* if selected 2 ``ListTableRow`` that contains "
"\"choices\" field with name \"tariff_type\" and values \"WAREHOUSE\" and "
"\"SLIDE\" respectively."
msgstr ""

